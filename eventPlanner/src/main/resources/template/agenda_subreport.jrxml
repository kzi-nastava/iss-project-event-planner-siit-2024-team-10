<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="AgendaSubreport"
              pageWidth="700"
              pageHeight="842"
              topMargin="0"
              columnWidth="535">

    <field name="itemName" class="java.lang.String"/>
    <field name="itemDescription" class="java.lang.String"/>
    <field name="startTime" class="java.lang.String"/>
    <field name="endTime" class="java.lang.String"/>
    <field name="itemLocation" class="java.lang.String"/>


    <detail>
        <band height="120">
            <textField>
                <reportElement x="0" y="0" width="200" height="20"/>
                <textElement>
                    <font isBold="true"/>
                </textElement>
                <textFieldExpression><![CDATA[$F{itemName}]]></textFieldExpression>
            </textField>
            <textField isStretchWithOverflow="true">
                <reportElement x="50" y="25" width="515" height="35"/>
                <textElement/>
                <textFieldExpression><![CDATA["Description: " + $F{itemDescription}]]></textFieldExpression>
            </textField>

            <textField>
                <reportElement x="50" y="50" width="100" height="20"/>
                <textElement/>
                <textFieldExpression><![CDATA["Start time: " + $F{startTime}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="50" y="75" width="100" height="20"/>
                <textElement/>
                <textFieldExpression><![CDATA["End time: " + $F{endTime}]]></textFieldExpression>
            </textField>
            <textField>
                <reportElement x="50" y="100" width="100" height="20"/>
                <textElement/>
                <textFieldExpression><![CDATA["Location: " + $F{itemLocation}]]></textFieldExpression>
            </textField>
        </band>
    </detail>
</jasperReport>